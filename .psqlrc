\set QUIET 1
\x auto
\timing

-- \set PROMPT1 '%m:%> %n%R%~%\n#~> '
-- \set PROMPT2 '%#=> '

\set PROMPT1 '%[%033[1;30m%]%m%[%033[0m%]%[%033[1;37m%]:%[%033[0m%]%[%033[1;30m%]%>%[%033[0m%] %[%033[1;32m%]%n%[%033[0m%]%[%033[1;37m%]%R%[%033[0m%]%[%033[1;33m%]%~%[%033[0m%]\n%[%033[1;34m%]#~> %[%033[0m%]'
\set PROMPT2 '%[%033[1;34m%]#->> %[%033[0m%]'
\set HISTFILE ~/.psql_history- :DBNAME
\set VERBOSITY verbose
\set ON_ERROR_ROLLBACK interactive
\set COMP_KEYWORD_CASE upper

\pset null '<<null>>'
\set QUIET 0
